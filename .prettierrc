{
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "printWidth": 100,
  "plugins": ["prettier-plugin-slidev"],
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "parser": "slidev",
        "slidevEntry": "./lessons/**/*.md"
      }
    }
  ]
}


